  "description": "A TLV string converter to JSON object",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "mocha",
    "lint": "eslint . --ext .ts",
    "prepare": "npm run build",
